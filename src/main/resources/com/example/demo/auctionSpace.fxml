<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.controler.AuctionMenuController" style="-fx-background-color: #89CFF0 ">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="show2"  visible="false"/>
    <Button fx:id="start" text="Start" onAction="#onStartClick" style="-fx-background-color: #dcd0ff"  />
    <ComboBox fx:id="comboBox" onAction="#onBoxClick" style="-fx-background-color: #ff6961" promptText="Select Auction Type"/>
    <Button fx:id="returnB" onAction="#onReturnButtonClick" style="-fx-background-color: #dcd0ff" text="Return"/>



</VBox>

