<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="349.0" prefWidth="530.0" spacing="20.0" style="-fx-background-color: #89CFF0 " xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="com.example.demo.controler.WorkSpaceController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Welcome in the first Chapter" />
    <TextField fx:id="writeNShow" promptText="Write in me " />
    <Button fx:id="show" onAction="#onShowButtonClick" style="-fx-background-color: #dcd0ff " text="Show me" />
    <Button fx:id="hide" onAction="#onHideButtonClick" style="-fx-background-color: #dcd0ff " text="Hide me" />
    <Button fx:id="alert" onAction="#onAlertButtonClickVIP" style="-fx-background-color: #dcd0ff " text="Alert" />
    <Button fx:id="returnB" onAction="#onReturnButtonClick" style="-fx-background-color: #dcd0ff " text="Log Out" />

    <Label fx:id="showText" style="-fx-font-family: 'Comic Sans MS' ; -fx-font-size: 15px" textFill="blueviolet" visible="false" />

</VBox>
